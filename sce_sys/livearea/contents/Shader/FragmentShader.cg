void main
(
    float2 TexCoord: TEXCOORD0,

    float4 out FragColor : COLOR,

    uniform sampler2D BoxTexture,
    uniform sampler2D FaceTexture
)
{
    float MixValue = 0.3;

    float4 BoxColor = float4(tex2D(BoxTexture, TexCoord).rgb , MixValue);

    float4 FaceColor = float4(tex2D(FaceTexture, TexCoord).rgb , (1.0 - MixValue));

    FragColor = BoxColor * FaceColor;
}